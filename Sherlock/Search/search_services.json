{
	"google": {
		"searchText": "Search with Google",
		"searchURL": "http://www.google.com/search?q={query}",
		"acURL": "http://suggestqueries.google.com/complete/search?client=firefox&q={query}",
		"icon": "google_icon.png"
	},

	"wikipedia": {
		"searchText": "Wikipedia",
		"searchURL": "https://en.wikipedia.org/w/index.php?search={query}",
		"categories": {"OrganizationName": 4, "PersonalName": 2, "PlaceName": 4},
		"icon": "wikipedia_icon.png",
		"acURL": "https://en.wikipedia.org/w/api.php?action=opensearch&search={query}&limit=1&namespace=0&format=json",
		"config": {
			"resultsPage": {
				"javascriptEnabled": true
			}
		}
	},

	"facebook": {
		"searchText": "Search Facebook",
		"searchURL": "https://www.facebook.com/search/top/?q={query}",
		"icon": "facebook_icon.png",
		"categories": {"PersonalName": 5},
		"config": {
			"resultsPage": {
				"javascriptEnabled": true
			}
		}
	},
    
    "duckduckgo": {
        "searchText": "Search with DuckDuckGo",
        "searchURL": "https://duckduckgo.com/?q={query}",
        "acURL": "http://duckduckgo.com/ac/?q={query}",
        "icon": "duckduckgo.png"
    },

    "twitter": {
    	"searchText": "Search Twitter",
    	"searchURL": "https://twitter.com/search?q={query}",
    	"categories": {"PersonalName": 5},
    	"icon": "twitter.png",
    	"config": {
    		"resultsPage": {
    			"javascriptEnabled": true
    		}
    	}
    },

    "linkedin":  {
    	"searchText": "Search LinkedIn",
    	"searchURL": "https://www.linkedin.com/search/results/all/?keywords={query}",
    	"categories": {"PersonalName": 2},	
    	"icon": "linkedin.png",
    	"config": {
    		"resultsPage": {
    			"javascriptEnabled": true
    		}
    	}
    },

    "applemaps": {
    	"searchText": "Search Apple Maps",
    	"searchURL": "http://maps.apple.com/?q={query}",
    	"categories": {"PlaceName": 3},
    	"icon": "applemaps.png",
    	"config": {
    		"resultsPage": {
    			"openURL": true,
    		}
    	}
    }
}
